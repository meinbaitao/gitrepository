@charset "utf-8";
html{width: 100%;height: 100%;}
body{background: #f0eff4;min-width:320px;max-width:540px;width: 100%;height: 100%;}
/**********header************/
.h_bg {
    background: url('../../common/images/test/bg.png') no-repeat;
    width: 100%;
    height: 6.5rem;
    text-align: center;
    color: #fff;
    padding: 1.6rem 0 1.4rem 0;
}
.h_img {
    width: 60px;
    height: 60px;
    margin: 0 auto;
    margin-bottom: 4px;
}
.h_img img {
    width: 60px;
    height: 60px;
    border-radius: 6px;
}
/**********container***********/
.container {
    background: #fff;
    border-top: 1px solid #e0e0e0;
}
.container>div {
    width: 94%;
    margin: 0 auto;
}
.p_data {
    border-bottom: 1px solid #e0e0e0;
    height: 22px;
    padding: 10px 0;
}
.data_h {
    width: 22px;
    height: 22px;
    margin: 0 7px 0 12px;
}
.data_m {
    height: 22px;
    line-height: 24px;
}
.p_data>div {
    float: left;
}
.c_back {
    margin: 0 auto;
    height: 46px;
    background: #FFB889;
    text-align: center;
    line-height: 46px;
    border-radius: 4px;
    width: 94%;
    color: #fff;
}
.c_back input {
    background: transparent;
    color: #fff;
    width: 100%;
    height: 100%;
}
.container .data_more {
    float: right;
    line-height: 24px;
    margin-right: 4px;
}

/**********footer***********/
@media screen and (max-width:399px) {
	.h_bg {
    font-size: 14px;
}
}
