@charset "utf-8";
html{width: 100%;height: 100%;}
body{min-width:320px;max-width:540px;width: 100%;height: 100%;}
/**********************************/
.container {
    width: 100%;
    background: #fff;
    height: auto;
}
.d_content {
    width: 93%;
    margin: 0 auto;
    overflow: hidden;
}
.d_title {
    width: 100%;
    white-space: nowrap;
    text-overflow: ellipsis;
    overflow: hidden;
    padding: 4px 0;
}
.d_date{padding: 4px 0;color:#999}
.d_cont {
    word-break: break-word;
    padding: 6px 0;color:#999
}