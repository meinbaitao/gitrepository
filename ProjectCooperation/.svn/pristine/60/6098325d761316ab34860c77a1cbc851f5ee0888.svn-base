/*main-container */
.main-container-scroll{	
	padding: 0 10px;
	width: 100%;
	height: 100%;
	overflow: auto;
}
.main-container{
	position:relative;
	padding: 0 320px 0px;
	width: 100%;
	height: 100%;
	overflow: hidden;
}
.main-container>div{
	float: left;
	border:1px solid #ccc;
	border-radius: 8px;
	box-shadow: 0 0 2px rgba(0,0,0,.132);
	background-color: #fff;
}
/*space-members*/
.space-members{
	padding:47px 0px 10px;
	margin: 0 0 0 -330px;
	height: 100%;
	width: 310px;

}
.space-members-header {
    margin: -49px 6px 0;
    padding: 0 0 0 9px;
    height: 47px;
    line-height: 46px;
    color: #30383b;
    font-size: 16px;
    border-bottom: 1px solid #ccc;
}
.space-members-header .sicon-setting{
	float: right;
	margin:16px 6px 0 0;
}

.team-name,.group-title{
    text-overflow: ellipsis;
    word-wrap: normal;
    white-space: nowrap;
    overflow:hidden;
}

/*\.space-members*/

/*task-container*/
.task-container-scroll {
    width: 100%;
    height: 100%;
    overflow: auto;
}
.task-container{
	padding:49px 0 0;
	width: 100%;
	height: 100%;
	min-width: 500px;
}
.my-task-header{
	padding:0 9px;
	margin: -52px 0 0;
	width: 100%;
	height: 48px;
	line-height: 47px;
	color: #30383b;
	font-size: 16px;
	text-indent: 5px;

}
.my-task-header .task-fliter{
	float: right;
	display: inline-block;
	margin: 0 5px 0 0;
	width: auto;
}
.task-fliter>.fliter-condition{
	font-size: 14px;
}
.task-fliter .dropdown-menu{
	position: absolute;
    right: -3px;
    top: 37px;
}
.task-fliter .dropdown-menu>.current{
   color: #ccc;
}
.shortcut-fliter{
	position:relative;
	margin: 0;
	padding-left: 12px;
	width: 100%;
	height: 0px;
	border-bottom: 1px solid #ccc;
	border-width:1px 0px;
}
.shortcut-fliter>li{
	position:relative;
	margin-left:8px; 
	color: #666;
	text-indent: 0px;
}
.display-select .shortcut-fliter>li.moveArea{
	position:absoulte;
	box-shadow:0 0 4px yellow;
	border-bottom:0px !important;
}
.shortcut-fliter>li>.number{
	display:none;
	position: absolute;
	margin: 4px 0 0 -6px;
	width: 16px;
	height: 16px;
	border-radius: 50%;
	color: #fff;
	line-height: 16px;
	font-size:12px;
	text-align: center; 
	text-indent: 0;
	background-color: #fcb593;
}
.shortcut-fliter>li>.number.has-mind{
	display:inline;
}
.shortcut-fliter>li .dropdown{
	position:absolute;
	width:24px;
	display:inline-block;
	right:-20px;
}
.shortcut-fliter>li .dropdown .dropdown-menu{
	margin-left:-68px;
}
/*\list-container*/
.list-container {
    padding: 0 9px;
    width: 100%;
    height: 100%;
    overflow: hidden;
    overflow-y: auto;
}
/*\.list-container*/

/*\.task-container*/

/*teams-list*/
.teams-list{
	margin: 0 -310px 0 10px ;
	padding:47px 9px 0;
	height: 100%;
	width: 300px;
}
.teams-list-head{
	position:relative;
	margin:-49px 0 0;
	width:100%;
	height:47px;
	line-height:46px;
	font-size:16px;
	color:#30383b;
	text-indent:5px;
	border-bottom: 1px solid #ccc;
}
.teams-list-head .sicon-addTeam{
	position:absolute;
	right:6px;
	top:15px;
}
.teams-list>.list{
	width:100%;
	height:100%;
	overflow:hidden;
}
.teams-list>.list>li{
	padding:12px 0;
	width:100%;
	height:auto;
	border-bottom:1px solid #ccc;
}
.teams-list>.list>li>.team-members{
	padding: 8px 0 0 8px;
    width: 100%;
    height: auto;
    line-height: initial;
}
.teams-list>.list>li>.team-members>span {
    margin: 4px 2px 0 0;
}
.list>li>.team-info{
	position:relative;
	width:100%;
	font-size:14px;
	padding:0 0 0 8px;
}
.list>li>.team-info>.team-name{
	display:inline-block;
	height:20px;
	line-height:20px;
	padding:0 40px 0 0;
	width:100%;
}
.list>li>.team-info>.dropdown{

	position:absolute;
    top:0px;
    right:9px;
	display:inline-block;

}
.list>li>.team-info>.dropdown>.dropdown-menu{
	margin: 0 0 0 -67px;
    min-width: 60px;
}
.list>li>.team-info>.team-date{
	width:100%;
	height:14px;
	line-height:14px;
	font-size:12px;
	color:#bdc8cc;
}
.list>li>.team-info>.team-date>span{
	padding-right:6px;
}
span.add-members-sign{
    position: absolute;
    top: 7px;
    right: 21px;
    font-size: 22px;
    color: #FDB494;
    border: 1px solid;
    border-radius: 50%;
    width: 23px;
    height: 23px;
    text-align: center;
    line-height: 20px;
    cursor: pointer;
}
#new_team .box-background{
	cursor: initial;
    background-color: #fff;
}
.sicon-hideMenu {
    display: inline-block;
    width: 18px;
    height: 125px;
    background: url('../new-images/sprite/icon-hideMenuNew.png') no-repeat;
}
.hideMenu-btn {
    position: absolute;
    top: 50%;
    right: -18px;
    margin-top: -64px;
    padding-right: 6px;
    font: 700 16px/122px "\5B8B\4F53";
    text-align: center;
    color: #b7b4a0;
    cursor: pointer;
    transition: text-align .2s;
}
.hideMenu-btn.sicon-hideMenu.project-eject:hover {
    font-size: 18px;
    text-align: left;
}
.space-members.put-away {
    position: relative;
}
.project-members-list .lists img {
    max-width: 100%;
    height: auto;
    width: 34px;
    border-radius: 50%;
}
#mentionMember .lists .info{
    position: absolute;
    width: auto;
    left: 49px;
    height: 50px;
    overflow: hidden;
    padding: 10px 0;
    top: 0;
}
span.team_members_sign {
    position: absolute;
    top: 6px;
    right: 22px;
    font-size: 22px;
    color: #FDB494;
    border: 1px solid;
    border-radius: 50%;
    width: 23px;
    height: 23px;
    text-align: center;
    line-height: 20px;
    cursor: pointer;
}
.main-container-scroll .main-container {
    padding: 0 310px 0 330px;
}
body{
	min-width: 1024px;
    overflow-x: auto;
}
#space-team-edit-modal .modal-dialog{
	width: 580px;
}
#space-team-edit-modal .project-member .member_list{
	
	height: 226px;
}
#space-team-edit-modal .tab-content{
	margin: 0;
}
#space-team-edit-modal .nav-tabs>li{
	margin-bottom: 0;
}
#space-team-edit-modal .nav-tabs>li{
	height: initial;
    line-height: initial;
}
#space-team-edit-modal .modal-header{
	height: initial;
}
#space-team-edit-modal .nav-tabs{
	border-bottom: initial;
}
#space-team-edit-modal .modal .nav-tabs li > a{
	border: none;
}
.modal .nav-tabs li > a,
.modal .nav-tabs:hover li.active > a{
	height: initial;
    line-height: initial;
}


#space-team-edit-modal .modal-header ul li a:a:focus{
	border-bottom: 2px solid #FF6100;
}
#space-team-edit-modal .modal-header ul li a {
    border-bottom: 2px solid transparent;
    background: #fff;
    border: none;
    color: #666;
}#space-team-edit-modal .modal-header ul li a:hover{
	color: #FF6100;
    border-bottom: 2px solid #FF6100;
    
}
#space-team-edit-modal .nav>li>a:focus,
#space-team-edit-modal .nav>li>a:hover
{
 	color: #FF6100;
    border-bottom: 2px solid #FF6100;
}
#space-team-edit-modal .modal-header ul li a, #space-team-edit-modal .modal-header ul li a:hover {
    border: none !important;
    color: #999 !important;
}











