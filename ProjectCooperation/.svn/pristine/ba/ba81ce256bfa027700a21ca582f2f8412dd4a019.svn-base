#add_project-charts .modal-body{
	overflow:visible;
}
.modal-dialog{
	position:absolute;
	top:0;
	bottom:0;
	left:0;
	right:0;
	margin:auto;
	width:500px;	
	height:290px;
}
.modal-content{
	 position:absolute;
	 top:0;
	 bottom:0;
	 left:0;
	 right:0;
	 margin:auto;
	 height:290px;
	 width:500px;
}
.modal-header{
	padding:0 0 0 16px;
	height:51px;
	line-height:50px;
	font-size:16px;
	color:#666;
	border-bottom: 1px solid #ccc;
}

.mymodal .modal-title {
    color: #596573;
    font-size: 17px;
}
.modal-body .form-group .control-label, .input-name {
    color: #999999;
    cursor: pointer;
}

.modal div.modal-dialog .modal-title span.sicon-setting{
	position:absolute;
	display:block;	
	left:0px;
	top:2px;
	margin-top:2px;
}
.modal .nav-tabs{
	display:inline-block;	
	margin: 1px 0  0 -10px;
	width:auto;
	border:0px;
	
}
.modal .nav-tabs li{
	margin-left:0px;
	margin-right:0px;
}
.modal .nav-tabs li>a,
.modal .nav-tabs:hover li.active>a{
	font:14px/21px "\5FAE\8F6F\96C5\9ED1";
	padding:2px 14px;
	cursor:pointer;
	color:#999999;
	border-width:0px 0px 2px 0px;
	border-color:transparent;
}
.modal .nav-tabs li.active>a,
.modal .nav-tabs li.active>a:hover,
.modal .nav-tabs li:hover>a:hover{	
    border-bottom: 2px solid #ff6000;
    color: #ff6000;
}
.modal .nav-tabs li>a:hover{
	background:transparent;
	border:0px;
}
.nav > li > a:focus, .nav > li > a:hover{
	background:transparent;
}

.modal .form-presentation{
	padding:0;
	font-size:14px;
	color:#58666e;
	font-family: "\5FAE\8F6F\96C5\9ED1";
}
.modal .form-group.portrait-line>label{
	padding-top:0px;
	
}
.modal .form-group.portrait-line img.portrait{
	width:60px;
	height:60px;
}

.modal .form-group.portrait-line input[type=button]{
	margin-top:12px;
	background-color:#fff;
	border:1px solid #ff6000;
	box-shadow:unset;
	color:#58666e;
	font-size: 14px;
}
.modal .form-presentation button.submit{
	width:100%;
	background-color:#23b7e5;
	border-width:0px;
}
.rows{
	width:100%;
	padding:5px 0px;
	 overflow: hidden;
}
.drag_div{
	position:fixed;
	top:100px;
	left:0px;
	min-width:120px;
	height:24px;
	line-height:24px;
	text-indent:5px;
	border:1px solid #999;
	border-radius:3px;
	background-color:#fff;
	opacity:0.7;
	overflow:hidden;	
	display:none;
	z-index:1009;
}
.drag_div.members{
	min-width:36px;
	width:36px;
	height:36px;
	line-height:36px;
	opacity:0.9;
	border-radius:50%;
	text-indent:0px;
}
label.control-label {
    margin-bottom: 0;
    padding-right: 0;
    text-align: left;
}
.toolbar{
	position: relative;
	width: 100%;
	height: 33px;
	padding:0px 0px;
	margin-top:-33px;
	color: #6d7981;
    font-size: 16px;
    font-weight: 400;
    border-radius: 0px 4px 0 0;  
}

.file-list{
	width:100%;
	height:auto;
	overflow:hidden;
}
.file-list.sicon-file20{
	width:100%;
	padding:0 0 0 28px;
	height:auto;
	overflow:hidden;
}
a.file-style{
	position:relative;
	display:inline-block;	
    padding: 0 13px;
    margin:0px 56px 8px 0;
	height:26px;
	width:auto;
	color:#333;
	line-height: 26px;
	border-radius:13px;
	background-color:#e3e3e3;	
	transition:all 0.2s ease 0s;
	overflow:hidden;
}

a.file-style:hover{
	padding-right:20px;
	background-color:#ccc 
}
a.file-style.haspreview:hover{
	padding-right:70px;
	margin-right:10px;
} 
a.file-style .preview{
	position:absolute;
	display:none;
	right:-100px;
	top:3px;
	width:46px;
	height:20px;
	color:#fff;
	line-height: 20px;
    text-align: center;
    border-radius: 10px;
	background-color:#fcb593;
	cursor:pointer;
}
a.file-style.haspreview:hover .preview{
	right:20px;
	display:inline-block;
}
a.file-style .sicon-remove{
	display:none;
}
a.file-style:hover .sicon-remove{
	position:absolute;
	top:8px;
	right:3px;
	display:inline-block;
}
