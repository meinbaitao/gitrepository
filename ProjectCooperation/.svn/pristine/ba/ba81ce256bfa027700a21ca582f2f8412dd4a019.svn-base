.operat-result-alert{
   z-index:8;
   position:absolute;
   top:200px;
   left:0;
   right:0;
   margin:auto;
   padding:13px 22px 10px 40px;
   width:240px;
   min-height:48px;
   line-height:20px;
   font-size:16px;
   color:#3edaa1;
   border:1px solid #c0ccd7;
   border-radius:8px;
   box-shadow:0 0 3px #d9d9d9;	
   background-color:#fff;
}
.operat-result-alert.failure{
	color:#f43737;
}
.icon-result-alert{
	display:block;
	position:absolute;
	top:50%;
	left:12px;
	margin-top:-10px;
	width:20px;
	height:20px;
	background:url('../images/sprite/operat-succse.png') no-repeat;
}
.operat-result-alert.failure .icon-result-alert{
	background:url('../images/sprite/operat-failure.png') no-repeat;
}
.close-operat-alert{
	display:block;
	position:absolute;
	top:50%;
	right:12px;
	margin-top:-5px;
	width:10px;
	height:10px;
	background:url('../images/sprite/close-operat-alert.png') no-repeat;
	cursor:pointer;
}
