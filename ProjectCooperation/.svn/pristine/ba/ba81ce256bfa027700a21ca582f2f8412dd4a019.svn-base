div.botton.col-sm-8{
	padding-right:0px;
}
div.botton>button{
	float:right;
	margin:0 0 0 8px;
}
.btn-orange,.btn-gray{
	float:right;
    display: inline-block;
	width:86px;
	height:40px;
	line-height:40px;
	font-size:14px;
	text-align:center;
	border-radius:5px;
	border:0px;
	background:#fff;
	outline:0px;
	cursor:pointer;
}
.btn-orange{
	color:#fff;
	background-color: #ff6000;
}
.btn-orange.disable{
	color:#ccc;
	background-color: #e7e7e7;
	cursor:not-allowed;
}
.btn-gray{
	border-radius:0;
	color:#999;
}	
