/* CSS Document */
/*reset CSS*/
html{
	-webkit-text-size-adjust:none 
}
html, body{width:100%;height:100%;}
html, body, div, span, object, iframe,h1, h2, h3, h4, h5, h6, p, blockquote, pre,abbr, address, cite, code,del, dfn, em, img, ins,kbd, q, samp,small, strong, sub, sup, var,b, i,dl, dt, dd, ol, ul, li,fieldset, form, label, legend,table, caption, tbody, tfoot,thead,tr, th, td,article, aside, canvas, details, figcaption, figure, footer, header, hgroup, menu, nav, section, summary,time, mark, audio, video {
    margin:0;
    padding:0;
    border:0;
    outline:0;
    font-size:100%;
    vertical-align:middle;
    background:transparent;
	outline-style:none;
	word-wrap: break-word;
}
.display-select{
	-moz-user-select: none; 
	-khtml-user-select: none; 
	user-select: none;
	-webkit-user-select:none; 
	-webkit-text-size-adjust:none;
	cursor:move !important;
	}
body.display-select *{
	cursor:move !important;
}
body.display-select ::-moz-selection{background:transparent; } 
body.display-select ::selection {background:transparent; } 
body.display-select .drag-container{
	box-shadow:0px 0px 10px #ff0 inset;
}
body.display-select #task_details.drag-container{
	box-shadow:0px 0px 10px #ff0;
}
body {
    line-height:1;
}
ul{
	list-style:none;
}
a{
    margin:0;
    padding:0;
    border:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
	
}
a:hover,a:focus{
	text-decoration:none;
	bblr:expression(this.onFocus=this.blur());/*IE使用*/
    outline-style:none;/*FF使用*/ 
	}
table {
    border-collapse:collapse;
    border-spacing:0;
}

input, select {
    vertical-align:middle;
}
input,textarea{
	outline: none; 
}
/*文字溢出自动省略*/
.space .space-name,
.members,
.project-title,
.tags,
.c-project-lists>li,
.member-groups-title .groups-name,
.member-groups li,
.add_members .email,
.space_lists>li>span,
.member-details >.members-info>.email,
.comment-info .comment-info-tags .comment-title,
.row>div,
.foot>span,
.one-claendar,
.spaces-nav-list span.title,
.space-nav>.spance-name,
.user-opreate .user-info .user-name,
.head .info .info-foot .sicon-localization,
.text-line-title .project-name > span,
.imgs-div.files a span.fiel-info span,
.text-line-tags .sicon-notification ,
.operate_tag.project-belong>.dropdown .project-eidt,
.info-list .belong-project,
.info-list .belong-space,
.text-line-grid .text-line-title textarea{
    text-overflow: ellipsis;
    word-wrap: normal;
    white-space: nowrap;
    overflow:hidden;
}
/*文字多行溢出自动省略*/
.spaces-container-nav .content{
  display:-webkit-box!important;
  overflow:hidden;
  text-overflow:ellipsis;
  work-break:break-all;
  -webkit-box-orient:vertical; /*方向*/
  -webkit-line-clamp:3; /*显示多少行文本*/
}
/*清除浮动*/
.task-operate:after,
.top-nav:after,
.panles-switc:after,
.comment-lists:after,
.comment-info:after,
.logtags:after,
.task-comment-input-submit:after,
.panels.conversations:after,
.c-project-list-head:after,
.start-comment-post:after,
.comment-list:after,
.comment-list .comment-info:after,
.comment-info .comment-info-tags:after,
.comment-input-div:after,
.panels.project-chart>div:after,
ul.space_lists li:after,
.member-details:after,
.task-group-div:after,
.space-container-head:after,
.spaces-container-nav:after,
.file-images:after,
.imgshowDiv:after,
.comment-input-content.spreaded.col:after,
.task-file-list:after,
.list-subject-operat:after,
.converdation-files-upload-progress:after{
	content:"";
	display:block;
	height:0;
	clear:both;
	visibility:hidden
	}
.task-operate,
.top-nav,
.panles-switc,
.comment-lists,
.comment-info,
.logtags,
.task-comment-input-submit,
.panels.conversations,
.c-project-list-head,
.start-comment-post,
.comment-list,
.comment-list .comment-info,
.comment-info .comment-info-tags,
.comment-input-div,
.panels.project-chart>div,
ul.space_lists li,
.member-details,
.task-group-div,
.space-container-head,
.spaces-container-nav,
.file-images,
.imgshowDiv,
.comment-input-content.spreaded.col,
.task-file-list,
.list-subject-operat,
.converdation-files-upload-progress{
