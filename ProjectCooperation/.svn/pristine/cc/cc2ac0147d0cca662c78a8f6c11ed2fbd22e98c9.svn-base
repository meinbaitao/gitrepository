 }
.ban_scroll{ overflow: hidden;}
.quick_mask{
	position: absolute;
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    z-index: 999;
    background: #333;
    opacity: 0.9;
    display:none;
    text-align: center;
    color:#fff;
}
.quick_mask ul {
    width: 210px;
    height: 24%;
    margin: 0 auto;
    margin-top: 156px;
}
.quick_mask ul li {
    width: 46px;
    float: left;
    padding: 5px 12px;
    margin-top: 1200px;
}
.m_close{
	font-size: 2rem;
    position: absolute;
    left: 48%;
    bottom: 11px;
}


.demo{
	width: 100%;
    height: auto;
    position: absolute;
    top: 0;
}
#f_gallery{ display:none;position: absolute;    
    top: 0;
    left: 0;
    right: 0;
    bottom: 0;
    background: #333;
    z-index: 999;
    background-size:100% !important;
}
.f_size{
	background-size:100% auto !important;
}
