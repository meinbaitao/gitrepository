html, body, div, span, object,h1, h2, h3, h4, h5, h6, p, blockquote, 
pre,abbr, address,  em, img, q, samp,small, 
strong, sub, sup,b, i,dl, dt, dd, ol, ul, li, form, label, 
legend,table, caption, tbody, tfoot,thead,tr, th, td,article, aside, canvas, footer, header, section{
    margin:0;
    padding:0;
    border:0;
    outline:0;
    font-size:100%;
    vertical-align:middle;
    background:transparent;
	outline-style:none;
	word-wrap:break-word;
}
html,body{
	margin:0px;
	padding:0px;
	width:100%;
	height:100%;
	font:14px/22px "\5FAE\8F6F\96C5\9ED1";
	font-size:14px;
	color:#333;
	background-color:#f2f2f2;
	overflow: hidden;
}
a{
    margin:0;
    padding:0;
    border:0;
    font-size:100%;
    vertical-align:baseline;
    background:transparent;
	
}
a:hover,a:focus{
	text-decoration:none;
	bblr:expression(this.onFocus=this.blur());/*IE使用*/
    outline-style:none;/*FF使用*/ 
	}
.display-select{
	-moz-user-select: none; 
	-khtml-user-select: none; 
	user-select: none;
	-webkit-user-select:none; 
	-webkit-text-size-adjust:none;
	cursor:move !important;
	}
body.display-select *{cursor:move !important}
body.display-select ::selection {
    color:#333;
    background:tranparent;
    text-shadow:none;
}
body.display-select ::-moz-selection {
    color:#333;
    background:tranparent;
    text-shadow:none;
}
ul{list-style:none;}
