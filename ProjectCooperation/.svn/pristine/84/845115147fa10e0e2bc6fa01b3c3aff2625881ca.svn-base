package com.thinkgem.jeesite.common.utils;


/**
 * 数值工具类
 * @author xiaocai
 * @version 2016-3-14
 */
public class MathUtils {
	
	/**
	 * 四舍五入取小数点后面两位
	 * @param m
	 * @return
	 */
	public static double convertDouble(double m) {
		long l1 = Math.round(m*100);
		double res = l1/100.0;
		return res;
	}
}
